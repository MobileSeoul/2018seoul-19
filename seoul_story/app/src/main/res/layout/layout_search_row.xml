<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
android:orientation="horizontal" android:layout_width="match_parent"
android:layout_height="match_parent">

    <TextView
        android:id="@+id/search_row_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:gravity="left|center_vertical"
        android:paddingLeft="@dimen/row_searchlist_padding"
        android:textColor="#000000"
        android:textSize="@dimen/row_searchlist_font_size" />

    <!--android:src="@drawable/ic_check_circle_regular"-->
    <ImageSwitcher
        android:id="@+id/search_row_check"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right|center_vertical"
        android:layout_marginRight="@dimen/row_searchlist_padding"/>


</FrameLayout>